{
    "name": "Dislexya",
    "version": "1.0.0",
    "description": "A reading extension to help dyslexic readers",
    "manifest_version": 3,
    "author": "Stephen Hunt",
    "action":{
        "default_title": "Dislexya"
    },
    "permissions": ["scripting", "activeTab"],
    "background": {
        "service_worker": "background.js"
    }
}